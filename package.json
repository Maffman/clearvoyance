{
  "name": "clearvoyance",
  "version": "0.0.1",
  "description": "",
  "main": "server/app.js",
  "files": [
    "client",
    "server",
    ".eslintrc"
  ],
  "scripts": {
    "start": "node server/app.js",
    "lint": "eslint \"client/**/*.js server/**/*.js\"",
    "build:js": "cat $(find client/ -name \\*.js) > dist/app.js",
    "prebuild:html": "rm $(find dist -name \\*.html) -f",
    "build:html": "cd client && cp --parents $(find . -name \\*.html) ../dist/ && cd ../",
    "build": "npm run build:js & npm run build:html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Maffman/clearvoyance.git"
  },
  "author": "The Hogbros",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Maffman/clearvoyance/issues"
  },
  "homepage": "https://github.com/Maffman/clearvoyance#readme",
  "dependencies": {
    "angular": "^1.4.3",
    "express": "^4.13.3"
  },
  "devDependencies": {
    "eslint": "^1.4.3"
  }
}
